{
    "name" : "pachno/b2db",
    "description" : "B2DB is a lightweight ORM for modern PHP applications",
    "keywords" : [
        "ORM", "Database", "db", "MariaDB", "MySQL", "Postgres"
    ],
    "authors" : [{
            "name" : "Daniel Andre Eikeland",
            "email" : "zegenie@gmail.com"
        }
    ],
    "license" : [
        "MPL-2.0"
    ],
    "require" : {
        "php" : "^7.4|^8.0|^8.1",
        "ext-Reflection" : "*",
        "ext-mbstring" : "*",
        "ext-pdo" : "*"
    },
    "suggest": {
        "ext-apcu": "APCu extension to allow apc caching"
    },
    "autoload" : {
        "psr-4" : {
            "b2db\\" : "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.88"
    }
}
